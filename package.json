{
  "name": "vue-infinite-grid",
  "version": "1.0.0",
  "description": "A vue component that can easily use egjs-infinitegrid's layouts.",
  "main": "dist/vue-infinite-grid.umd.js",
  "module": "dist/vue-infinite-grid.esm.js",
  "unpkg": "dist/vue-infinite-grid.min.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config config/rollup.config.browser.js",
    "build:es": "rollup --config config/rollup.config.es.js",
    "build:umd": "rollup --config config/rollup.config.umd.js",
    "prepublish": "npm run test && npm run build",
    "dev": "cross-env NODE_ENV=development rollup --config config/rollup.config.es.js --watch",
    "test": "npm run test:unit",
    "test:unit": "jest --config tests/unit/jest.config.js --coverage"
  },
  "author": {
    "name": "Sel√ßuk Kiraz <selcukkiraz@gmail.com>"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/celljuke/vue-infinite-grid.git"
  },
  "keywords": [
    "vue",
    "infinitegrid"
  ],
  "dependencies": {
    "@egjs/infinitegrid": "^3.2.4"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.16.0",
    "clean-css": "^4.1.8",
    "cross-env": "^5.0.5",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.3.0",
    "jest-vue": "^0.8.2",
    "rollup": "^0.51.8",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.4.1",
    "uglify-es": "^3.3.10",
    "vue-loader": "^14.2.1",
    "vue-server-renderer": "^2.5.16",
    "vue-test-utils": "^1.0.0-beta.4"
  }
}
